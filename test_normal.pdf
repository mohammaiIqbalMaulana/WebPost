            background: linear-gradient(135deg, #fffdf5 0%, #fff3cd 100%);
        }
    </style>
    <script>
        window.addEventListener('load', () => {
            // Fungsi untuk mengatur bar progress
            const setProgressBar = (progressBar, percentage, actualChange) => {
                if (!progressBar) return;
                
                // Reset classes
                progressBar.classList.remove('bg-success', 'bg-danger', 'bg-warning');
                
                if (actualChange === 0) {
                    // Jika tidak ada perubahan (0)
                    progressBar.style.width = '100%';
                    progressBar.classList.add('bg-warning');
                } else if (actualChange < 0) {
                    // Jika negatif (penurunan)
                    progressBar.style.width = '100%';
                    progressBar.classList.add('bg-danger');
                } else {
                    // Jika positif (kenaikan)
                    progressBar.style.width = percentage + '%';
                    progressBar.classList.add('bg-success');
                }
            };

            // Animasi untuk progress bar followers
            const followerBars = document.querySelectorAll('.progress-bar[data-follower-progress]');
            followerBars.forEach(bar => {
                const progress = parseFloat(bar.getAttribute('data-follower-progress'));
                const change = parseFloat(bar.getAttribute('data-change') || '0');
                setTimeout(() => setProgressBar(bar, Math.abs(progress), change), 100);
            });

            // Animasi untuk progress bar insight metrics
            const metricBars = document.querySelectorAll('.progress-bar[data-metric-progress]');
            metricBars.forEach(bar => {
                const progress = parseFloat(bar.getAttribute('data-metric-progress'));
                const change = parseFloat(bar.getAttribute('data-change') || '0');
                setTimeout(() => setProgressBar(bar, Math.abs(progress), change), 100);
            });
        });
    </script>
